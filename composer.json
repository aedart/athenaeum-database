{
    "name": "aedart/athenaeum-database",
    "description": "Database utilities for Laravel",
    "keywords": [
        "Athenaeum",
        "Database",
        "Db"
    ],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aedart/athenaeum-contracts": "^7.0",
        "aedart/athenaeum-support": "^7.0",
        "aedart/athenaeum-utils": "^7.0",
        "illuminate/database": "^v10.0.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Database\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
