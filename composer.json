{
    "name": "aedart/athenaeum-database",
    "description": "Database utilities for Laravel",
    "keywords": ["Athenaeum", "Database", "Db"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-contracts": "dev-main",
        "aedart/athenaeum-support": "dev-main",
        "aedart/athenaeum-utils": "dev-main",
        "illuminate/database": "v9.0.1"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\Database\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
